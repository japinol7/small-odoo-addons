<?xml version="1.0" encoding="utf-8"?>
<odoo>
<record id="jap_addons_config_form_view" model="ir.ui.view">
    <field name="name">jap.addons.config.form.inherit.security</field>
    <field name="model">jap.addons.config</field>
    <field name="inherit_id" ref="jap_addons_config.jap_addons_config_form_view"/>
    <field name="arch" type="xml">
        <xpath expr="//page[@name='notes']" position="after">
                <page string="Wizards config" name="security_config">
                    <separator string="Create Invoice From Lines Wizard" colspan="4" style="color:#3b3b9b"/>
                    <group name="group1">
                        <field name="prefix_msg_create_invoice_sel_wiz"
                               string="Prefix Msg Create Invoice"/>
                    </group>
                </page>
        </xpath>
    </field>
</record>
</odoo>
